<Window 
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MEP-Räume-Übersicht" Height="800" Width="1500" Background="SkyBlue">
    <Grid Margin="10">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="400"/>
            <ColumnDefinition Width="0.5*" MinWidth="160"/>
            <ColumnDefinition Width="0.5*" MinWidth="160"/>
            <ColumnDefinition Width="0.5*" MinWidth="160"/>
            <ColumnDefinition Width="0.5*" MinWidth="160"/>
            <ColumnDefinition Width="350"/>

        </Grid.ColumnDefinitions>

        <Grid.RowDefinitions>
            <RowDefinition Height="180"/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition Height="0.8*"/>
            <RowDefinition Height="50"/>

        </Grid.RowDefinitions>

        <GroupBox VerticalAlignment="Center" Margin="10,0" Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="5">
            <GroupBox.Header>
                <TextBlock Text="MEP-Raum" FontWeight="Bold"/>
            </GroupBox.Header>
            <DockPanel>
                <GroupBox Header="Raumfinder" Margin="5,5,5,5">
                    <StackPanel>
                        <DockPanel Height="20" VerticalAlignment="Center" Margin="5,5,5,5">
                            <TextBlock Text="Filter: " VerticalAlignment="Center" Margin="0,0,0,0" Foreground="#FFAE4141" FontWeight="Bold" ToolTip="Nummer" Width="40"/>
                            <TextBox x:Name="suche" VerticalAlignment="Center" Width="290" TextChanged="MEP_Suche_changed"  />
                        </DockPanel>
                        <DockPanel Height="20" VerticalAlignment="Center" Margin="5,5,5,5">
                            <TextBlock Text="Raum: " VerticalAlignment="Center" Margin="0,0,0,0" Foreground="#FFAE4141" FontWeight="Bold" ToolTip="Nummer" Width="40"/>
                            <ComboBox x:Name="Raumnr" IsEditable="True" VerticalAlignment="Center" Width="290" SelectionChanged="MEP_changed"/>
                        </DockPanel>
                        <Button x:Name="rauman" Click="zeigraum" Content="Raum anzeigen" VerticalAlignment="Center" Margin="5,5,5,0" FontWeight="Bold" ToolTip="Raum anzeigen" IsEnabled="False"/>
                    </StackPanel>

                </GroupBox>
                <GroupBox Header="Grundinfo." Margin="5,5,5,5">
                    <StackPanel>
                        <DockPanel Height="20" Margin="5,0,5,0">
                            <TextBlock Text="Ebene: " VerticalAlignment="Center" Margin="0,0,5,0" Foreground="#FFAE4141" FontWeight="Bold" ToolTip="Ebene" />
                            <TextBlock x:Name="ebene" HorizontalAlignment="Left" VerticalAlignment="Center" Width="100"/>
                        </DockPanel>
                        <DockPanel Height="20" Margin="5,0,5,0">
                            <TextBlock VerticalAlignment="Center" Margin="0,0,5,0" Foreground="#FFAE4141" FontWeight="Bold" Text="Fläche: " Width="70"/>
                            <TextBlock x:Name="flaeche" VerticalAlignment="Center" Width="50" />
                            <TextBlock Text=" m²" VerticalAlignment="Center" Margin="0" />
                        </DockPanel>
                        <DockPanel Height="20" Margin="5,0,5,0">
                            <TextBlock VerticalAlignment="Center" Margin="0,0,5,0" Foreground="#FFAE4141" FontWeight="Bold" Text="Volumen: " Width="70"/>
                            <TextBlock x:Name="volumen" VerticalAlignment="Center" Width="50" />
                            <TextBlock Text=" m³" VerticalAlignment="Center" Margin="0" />
                        </DockPanel>
                        <DockPanel Height="20" Margin="0,0,5,0">
                            <TextBlock VerticalAlignment="Center" Margin="5,0,5,0" Foreground="#FFAE4141" FontWeight="Bold" Text="Licht Höhe:" Width="70"/>
                            <TextBlock x:Name="hoehe" VerticalAlignment="Center" Width="50"/>
                            <TextBlock Text=" mm" VerticalAlignment="Center" Margin="0" />
                        </DockPanel>
                        <DockPanel Height="20" Margin="0,0,5,0">
                            <TextBlock VerticalAlignment="Center" Margin="5,0,5,0" Foreground="#FFAE4141" FontWeight="Bold" Text="Personen: " Width="70"/>
                            <TextBlock x:Name="personen" VerticalAlignment="Center" />
                        </DockPanel>

                    </StackPanel>
                </GroupBox>
                <GroupBox Header="Tagesbetrieb" Margin="5,5,5,5">
                    <StackPanel Margin="5,0,5,0">
                        <TextBlock Text="Berechnung nach: " VerticalAlignment="Center" Foreground="#FFAE4141" FontWeight="Bold" ToolTip="TGA_RLT_VolumenstromProName" Margin="0,5,0,0"/>
                        <ComboBox x:Name="bezugsname" IsEnabled="False" VerticalAlignment="Center" SelectionChanged="bezugsnameselectionchanged" IsEditable="True" Width="130" Margin="0,5,0,0"/>
                        <TextBlock Text="Faktor:" VerticalAlignment="Center" Foreground="#FFAE4141" FontWeight="Bold" ToolTip="TGA_RLT_VolumenstromProFaktor" Margin="0,5,0,0"/>
                        <DockPanel>
                            <TextBox x:Name="faktor" IsEnabled="False" VerticalAlignment="Center" Margin="0,5,0,5" Width="50" HorizontalAlignment="Stretch" VerticalContentAlignment="Center"/>
                            <TextBlock x:Name="einheit" VerticalAlignment="Center" />
                        </DockPanel>
                    </StackPanel>
                </GroupBox>
                <GroupBox Header="Nachtsbetrieb" Margin="5,5,5,5">
                    <StackPanel>
                        <DockPanel Margin="5,5,5,5">
                            <TextBlock Text="Nachtsbetrieb:" VerticalAlignment="Center" Foreground="#FFAE4141" FontWeight="Bold" ToolTip="IGF_RLT_Nachtbetrieb"/>
                            <CheckBox x:Name="isnachtbetrieb" IsEnabled="False" Click="nachtbetriebchanged" Margin="5,0,0,0" VerticalContentAlignment="Center"/>
                        </DockPanel>
                        <DockPanel Margin="5,5,5,5">
                            <TextBlock Text="Luftwechsel:" VerticalAlignment="Center" Foreground="#FFAE4141" FontWeight="Bold" ToolTip="IGF_RLT_Nachtbetrieb" Width="75" Height="18"/>
                            <TextBox x:Name="LW_nacht" IsEnabled="False" Width="25" Height="18" TextChanged="LW_nacht_changed"/>
                            <TextBlock Text="-1/h" Margin="5,0,0,0"/>
                        </DockPanel>
                        <DockPanel Margin="5,5,5,5">
                            <TextBlock Text="Von:" VerticalAlignment="Center" Foreground="#FFAE4141" FontWeight="Bold" ToolTip="IGF_RLT_Nachtbetrieb" Width="75" Height="18"/>
                            <TextBox x:Name="von_nacht" Height="18" IsEnabled="False"/>
                        </DockPanel>
                        <DockPanel Margin="5,5,5,5">
                            <TextBlock Text="Bis:" VerticalAlignment="Center" Foreground="#FFAE4141" FontWeight="Bold" ToolTip="IGF_RLT_Nachtbetrieb" Width="75" Height="18"/>
                            <TextBox x:Name="bis_nacht" Height="18" IsEnabled="False"/>
                        </DockPanel>
                    </StackPanel>
                </GroupBox>
                <GroupBox Header="TieferNachtsbetrieb" Margin="5,5,5,5">
                    <StackPanel>
                        <DockPanel Margin="5,5,5,5">
                            <TextBlock Text="TieferNachtsbetrieb:" VerticalAlignment="Center" Foreground="#FFAE4141" FontWeight="Bold" ToolTip="IGF_RLT_Nachtbetrieb"/>
                            <CheckBox x:Name="istiefernachtbetrieb" Click="tiefernachtbetriebchanged" IsEnabled="False" Margin="3,0,0,0" VerticalContentAlignment="Center"/>
                        </DockPanel>
                        <DockPanel Margin="5,5,5,5">
                            <TextBlock Text="Luftwechsel:" VerticalAlignment="Center" Foreground="#FFAE4141" FontWeight="Bold" ToolTip="IGF_RLT_Nachtbetrieb" Width="75" Height="18"/>
                            <TextBox x:Name="LW_tiefernacht" Width="25" Height="18" IsEnabled="False" TextChanged="LW_tiefernacht_changed"/>
                            <TextBlock Text="-1/h" Margin="5,0,0,0"/>
                        </DockPanel>
                        <DockPanel Margin="5,5,5,5">
                            <TextBlock Text="Von:" VerticalAlignment="Center" Foreground="#FFAE4141" FontWeight="Bold" ToolTip="IGF_RLT_Nachtbetrieb" Width="75" Height="18"/>
                            <TextBox x:Name="von_tiefernacht" Height="18" IsEnabled="False"/>
                        </DockPanel>
                        <DockPanel Margin="5,5,5,5">
                            <TextBlock Text="Bis:" VerticalAlignment="Center" Foreground="#FFAE4141" FontWeight="Bold" ToolTip="IGF_RLT_Nachtbetrieb" Width="75" Height="18"/>
                            <TextBox x:Name="bis_tiefernacht" Height="18" IsEnabled="False"/>
                        </DockPanel>
                    </StackPanel>
                </GroupBox>
                <StackPanel/>
            </DockPanel>
        </GroupBox>
        <StackPanel Grid.Column="5" x:Name="warnung" Margin="0,15,0,0">
            <TextBlock Text="Achtung: Der VSR versorgt mehrere Räume!" Foreground="#FFFF0505" TextWrapping="Wrap" VerticalAlignment="Top" Margin="10,0" FontSize="14"/>
            <TextBlock Text="-------------------------------------------------------------" VerticalAlignment="Top" Margin="10,0" FontSize="14"/>
            <TextBlock x:Name="Rauminfo" Text="Die angezeigte Rauminfomationen beziehen sich auf Raum =>" TextWrapping="Wrap" Visibility="Visible" FontSize="14" Foreground="#FFF5085E" HorizontalAlignment="Stretch" VerticalAlignment="Center" Padding="10,0,0,0"/>
            <StackPanel Height="40"  Margin="10,0,10,10"  VerticalAlignment="Bottom">
                <TextBlock x:Name="raumwechsel_hinweis" Text="Hier den Raum wechseln:"/>
                <ComboBox x:Name="auswahl"  SelectionChanged="VSR_MEPRaum_changed"/>
            </StackPanel>
        </StackPanel>

        <DockPanel Margin="10,5,5,10" Grid.Row="1" Grid.RowSpan="2">
            <TabControl  Background="LightBlue">
                <TabItem >
                    <TabItem.Header>
                        <TextBlock Text="Übersicht" FontWeight="Bold"/>
                    </TabItem.Header>
                    <DataGrid x:Name="grundinfo" Background="LightBlue" ItemsSource="{Binding}" CanUserSortColumns="False">
                        <DataGrid.CellStyle>
                            <Style TargetType="{x:Type DataGridCell}">
                                <Setter Property="ToolTip" Value="{Binding tooltip}"/>
                                <Style.Triggers>
                                    <DataTrigger Binding="{Binding name}" Value="Zuluft min.">
                                        <Setter Property="Foreground" Value="Blue"/>
                                    </DataTrigger>
                                    <DataTrigger Binding="{Binding name}" Value="Zuluft max.">
                                        <Setter Property="Foreground" Value="Blue"/>
                                    </DataTrigger>
                                    <DataTrigger Binding="{Binding name}" Value="Abluft min.">
                                        <Setter Property="Foreground" Value="Orange"/>
                                    </DataTrigger>
                                    <DataTrigger Binding="{Binding name}" Value="Abluft max.">
                                        <Setter Property="Foreground" Value="Orange"/>
                                    </DataTrigger>
                                    <DataTrigger Binding="{Binding name}" Value="24h Abluft">
                                        <Setter Property="Foreground" Value="#FFC85B00"/>
                                    </DataTrigger>
                                    <DataTrigger Binding="{Binding name}" Value="LAB min.">
                                        <Setter Property="Foreground" Value="orange"/>
                                    </DataTrigger>
                                    <DataTrigger Binding="{Binding name}" Value="LAB max.">
                                        <Setter Property="Foreground" Value="Orange"/>
                                    </DataTrigger>
                                    <DataTrigger Binding="{Binding name}" Value="Überstrom">
                                        <Setter Property="Foreground" Value="Red"/>
                                    </DataTrigger>
                                    <DataTrigger Binding="{Binding name}" Value="Über. Ein.">
                                        <Setter Property="Foreground" Value="Red"/>
                                    </DataTrigger>
                                    <DataTrigger Binding="{Binding name}" Value="Über. Aus.">
                                        <Setter Property="Foreground" Value="Red"/>
                                    </DataTrigger>
                                    <DataTrigger Binding="{Binding name}" Value="NB Zuluft">
                                        <Setter Property="Foreground" Value="Blue"/>
                                    </DataTrigger>
                                    <DataTrigger Binding="{Binding name}" Value="TNB Zuluft">
                                        <Setter Property="Foreground" Value="Blue"/>
                                    </DataTrigger>
                                    <DataTrigger Binding="{Binding name}" Value="NB Abluft">
                                        <Setter Property="Foreground" Value="Orange"/>
                                    </DataTrigger>
                                    <DataTrigger Binding="{Binding name}" Value="TNB Abluft">
                                        <Setter Property="Foreground" Value="Orange"/>
                                    </DataTrigger>
                                </Style.Triggers>
                            </Style>
                        </DataGrid.CellStyle>
                        <DataGrid.Columns>
                            <DataGridTextColumn IsReadOnly="True" Header="Art" Width="*" Binding="{Binding name}"/>
                            <DataGridTextColumn IsReadOnly="True" Header="Soll (m³/h)" Width="*" Binding="{Binding soll}"/>
                            <DataGridTextColumn IsReadOnly="True" Header="Ist (m³/h)" Width="*" Binding="{Binding ist}"/>
                        </DataGrid.Columns>
                    </DataGrid>
                </TabItem>
                <TabItem >
                    <TabItem.Header>
                        <TextBlock Text="Auswertung" FontWeight="Bold"/>
                    </TabItem.Header>
                    <TabControl  Background="LightBlue">
                        <TabItem Header="min"  Width="50">
                            <StackPanel Background="White" Margin="0,0,0,0">
                                <DockPanel Margin="0,5,0,0" >
                                    <TextBlock Text="" FontWeight="Bold" Width="100" Foreground="Blue"/>
                                    <TextBlock  FontWeight="Bold" Text="MEP" Width="100" />
                                    <TextBlock FontWeight="Bold" Text="Anlagen" Width="100" />
                                </DockPanel>
                                <Rectangle StrokeThickness="2" Stroke="Black" OpacityMask="Black"></Rectangle>
                                <DockPanel Margin="0,5,0,0" >
                                    <TextBlock Text="Zuluft-Summe: " FontWeight="Bold" Width="100" Foreground="Blue"/>
                                    <TextBlock x:Name="b_zu_min_sum_mep" FontWeight="Bold" Width="100" Foreground="Blue"/>
                                    <TextBlock x:Name="b_zu_min_sum_sys" FontWeight="Bold" Width="100" Foreground="Blue"/>
                                </DockPanel>
                                <Rectangle StrokeThickness="1" Stroke="Black" OpacityMask="Black"></Rectangle>
                                <DockPanel Margin="0,5,0,0" >
                                    <TextBlock Text="Zu-Raum: " Width="100"  Foreground="Blue"/>
                                    <TextBlock x:Name="b_zu_min_raum_mep" Width="100" Foreground="Blue"/>
                                    <TextBlock x:Name="b_zu_min_raum_sys" Width="100"   Foreground="Blue"/>
                                </DockPanel>
                                <Rectangle StrokeThickness="1" Stroke="Black" OpacityMask="Black"></Rectangle>
                                <DockPanel Margin="0,2,0,0" >
                                    <TextBlock Text="Zu-Tierhaltung: " Width="100"  Foreground="Blue"/>
                                    <TextBlock x:Name="b_zu_min_tier_mep" Width="100"  Foreground="Blue"/>
                                    <TextBlock x:Name="b_zu_min_tier_sys" Width="100" Foreground="Blue"/>
                                </DockPanel>
                                <Rectangle StrokeThickness="1" Stroke="Black" OpacityMask="Black"></Rectangle>
                                <DockPanel Margin="0,2,0,0" >
                                    <TextBlock Text="Zu-Überstrom: " Width="100"  Foreground="Blue"/>
                                    <TextBlock x:Name="b_zu_min_ueber_mep" Width="100" Foreground="Blue"/>
                                    <TextBlock x:Name="b_zu_min_ueber_sys" Width="100" Foreground="Blue"/>
                                </DockPanel>
                                <Rectangle StrokeThickness="2" Stroke="Black" OpacityMask="Black"></Rectangle>

                                <DockPanel Margin="0,10,0,0" >
                                    <TextBlock Text="Abluft-Summe: " FontWeight="Bold" Width="100"  Foreground="Orange"/>
                                    <TextBlock x:Name="b_ab_min_sum_mep" FontWeight="Bold" Width="100" Foreground="Orange"/>
                                    <TextBlock x:Name="b_ab_min_sum_sys" FontWeight="Bold" Width="100" Foreground="Orange"/>
                                </DockPanel>
                                <Rectangle StrokeThickness="1" Stroke="Black" OpacityMask="Black"></Rectangle>
                                <DockPanel Margin="0,5,0,0" >
                                    <TextBlock Text="Ab-Raum: " Width="100" Foreground="Orange"/>
                                    <TextBlock x:Name="b_ab_min_raum_mep" Width="100"  Foreground="Orange"/>
                                    <TextBlock x:Name="b_ab_min_raum_sys" Width="100"  Foreground="Orange"/>
                                </DockPanel>
                                <Rectangle StrokeThickness="1" Stroke="Black" OpacityMask="Black"></Rectangle>
                                <DockPanel Margin="0,2,0,0" >
                                    <TextBlock Text="Ab-Tierhaltung: " Width="100"  Foreground="Orange"/>
                                    <TextBlock x:Name="b_ab_min_tier_mep" Width="100" Foreground="Orange"/>
                                    <TextBlock x:Name="b_ab_min_tier_sys" Width="100" Foreground="Orange"/>
                                </DockPanel>
                                <Rectangle StrokeThickness="1" Stroke="Black" OpacityMask="Black"></Rectangle>
                                <DockPanel Margin="0,2,0,0" >
                                    <TextBlock Text="Ab-Überstrom: " Width="100" Foreground="Orange"/>
                                    <TextBlock x:Name="b_ab_min_ueber_mep" Width="100" Foreground="Orange"/>
                                    <TextBlock x:Name="b_ab_min_ueber_sys" Width="100" Foreground="Orange"/>
                                </DockPanel>
                                <Rectangle StrokeThickness="1" Stroke="Black" OpacityMask="Black"></Rectangle>
                                <DockPanel Margin="0,2,0,0" >
                                    <TextBlock Text="Ab-Labor: " Width="100" Foreground="Orange"/>
                                    <TextBlock x:Name="b_ab_min_lab_mep" Width="100" Foreground="Orange"/>
                                    <TextBlock x:Name="b_ab_min_lab_sys" Width="100" Foreground="Orange"/>
                                </DockPanel>
                                <Rectangle StrokeThickness="1" Stroke="Black" OpacityMask="Black"></Rectangle>
                                <DockPanel Margin="0,2,0,0" >
                                    <TextBlock Text="Ab-24h: " Width="100" Foreground="Orange"/>
                                    <TextBlock x:Name="b_ab_24h_min_mep" Width="100" Foreground="Orange"/>
                                    <TextBlock x:Name="b_ab_24h_min_sys" Width="100" Foreground="Orange"/>
                                </DockPanel>
                                <Rectangle StrokeThickness="2" Stroke="Black" OpacityMask="Black"></Rectangle>
                                <DockPanel Margin="0,10,0,0" >
                                    <TextBlock Text="Luftbilanz:" Width="100" FontWeight="Bold"  FontSize="14"/>
                                    <TextBlock x:Name="min_Abweichung_mep" Width="100"  FontWeight="Bold"  FontSize="14"/>
                                    <TextBlock x:Name="min_Abweichung_sys" Width="100"  FontWeight="Bold"  FontSize="14"/>
                                </DockPanel>
                                <Rectangle StrokeThickness="2" Stroke="Black" OpacityMask="Black"></Rectangle>
                                <DockPanel Margin="0,10,0,0" >
                                    <TextBlock Text="Druckstufe: " Width="100" FontWeight="Bold"  FontSize="14"/>
                                    <TextBlock x:Name="min_auswertung_druckstufe" Width="100"  FontWeight="Bold"  FontSize="14"/>
                                </DockPanel>
                                <Rectangle StrokeThickness="2" Stroke="Black" OpacityMask="Black"></Rectangle>
                                <DockPanel Margin="0,10,0,0" >
                                    <TextBlock Text="Auswertung: " Width="100" FontWeight="Bold"  FontSize="14"/>
                                    <TextBlock x:Name="min_auswertung_mep" Width="100"  FontWeight="Bold"  FontSize="14"/>
                                    <TextBlock x:Name="min_auswertung_sys" Width="100"  FontWeight="Bold"  FontSize="14"/>
                                </DockPanel>

                            </StackPanel>




                        </TabItem>
                        <TabItem Header="max" Width="50">
                            <StackPanel Background="White" Margin="0,0,0,0">
                                <DockPanel Margin="0,5,0,0" >
                                    <TextBlock Text="" FontWeight="Bold" Width="100" Foreground="Blue"/>
                                    <TextBlock  FontWeight="Bold" Text="MEP" Width="100" />
                                    <TextBlock FontWeight="Bold" Text="Anlagen" Width="100" />
                                </DockPanel>
                                <Rectangle StrokeThickness="2" Stroke="Black" OpacityMask="Black"></Rectangle>
                                <DockPanel Margin="0,5,0,0" >
                                    <TextBlock Text="Zuluft-Summe: " FontWeight="Bold" Width="100" Foreground="Blue"/>
                                    <TextBlock x:Name="b_zu_max_sum_mep" FontWeight="Bold" Width="100" Foreground="Blue"/>
                                    <TextBlock x:Name="b_zu_max_sum_sys" FontWeight="Bold" Width="100" Foreground="Blue"/>
                                </DockPanel>
                                <Rectangle StrokeThickness="1" Stroke="Black" OpacityMask="Black"></Rectangle>
                                <DockPanel Margin="0,5,0,0" >
                                    <TextBlock Text="Zu-Raum: " Width="100"  Foreground="Blue"/>
                                    <TextBlock x:Name="b_zu_max_raum_mep" Width="100" Foreground="Blue"/>
                                    <TextBlock x:Name="b_zu_max_raum_sys" Width="100"   Foreground="Blue"/>
                                </DockPanel>
                                <Rectangle StrokeThickness="1" Stroke="Black" OpacityMask="Black"></Rectangle>
                                <DockPanel Margin="0,2,0,0" >
                                    <TextBlock Text="Zu-Tierhaltung: " Width="100"  Foreground="Blue"/>
                                    <TextBlock x:Name="b_zu_max_tier_mep" Width="100"  Foreground="Blue"/>
                                    <TextBlock x:Name="b_zu_max_tier_sys" Width="100" Foreground="Blue"/>
                                </DockPanel>
                                <Rectangle StrokeThickness="1" Stroke="Black" OpacityMask="Black"></Rectangle>
                                <DockPanel Margin="0,2,0,0" >
                                    <TextBlock Text="Zu-Überstrom: " Width="100"  Foreground="Blue"/>
                                    <TextBlock x:Name="b_zu_max_ueber_mep" Width="100" Foreground="Blue"/>
                                    <TextBlock x:Name="b_zu_max_ueber_sys" Width="100" Foreground="Blue"/>
                                </DockPanel>
                                <Rectangle StrokeThickness="2" Stroke="Black" OpacityMask="Black"></Rectangle>

                                <DockPanel Margin="0,10,0,0" >
                                    <TextBlock Text="Abluft-Summe: " FontWeight="Bold" Width="100"  Foreground="Orange"/>
                                    <TextBlock x:Name="b_ab_max_sum_mep" FontWeight="Bold" Width="100" Foreground="Orange"/>
                                    <TextBlock x:Name="b_ab_max_sum_sys" FontWeight="Bold" Width="100" Foreground="Orange"/>
                                </DockPanel>
                                <Rectangle StrokeThickness="1" Stroke="Black" OpacityMask="Black"></Rectangle>
                                <DockPanel Margin="0,5,0,0" >
                                    <TextBlock Text="Ab-Raum: " Width="100" Foreground="Orange"/>
                                    <TextBlock x:Name="b_ab_max_raum_mep" Width="100"  Foreground="Orange"/>
                                    <TextBlock x:Name="b_ab_max_raum_sys" Width="100"  Foreground="Orange"/>
                                </DockPanel>
                                <Rectangle StrokeThickness="1" Stroke="Black" OpacityMask="Black"></Rectangle>
                                <DockPanel Margin="0,2,0,0" >
                                    <TextBlock Text="Ab-Tierhaltung: " Width="100"  Foreground="Orange"/>
                                    <TextBlock x:Name="b_ab_max_tier_mep" Width="100" Foreground="Orange"/>
                                    <TextBlock x:Name="b_ab_max_tier_sys" Width="100" Foreground="Orange"/>
                                </DockPanel>
                                <Rectangle StrokeThickness="1" Stroke="Black" OpacityMask="Black"></Rectangle>
                                <DockPanel Margin="0,2,0,0" >
                                    <TextBlock Text="Ab-Überstrom: " Width="100" Foreground="Orange"/>
                                    <TextBlock x:Name="b_ab_max_ueber_mep" Width="100" Foreground="Orange"/>
                                    <TextBlock x:Name="b_ab_max_ueber_sys" Width="100" Foreground="Orange"/>
                                </DockPanel>
                                <Rectangle StrokeThickness="1" Stroke="Black" OpacityMask="Black"></Rectangle>
                                <DockPanel Margin="0,2,0,0" >
                                    <TextBlock Text="Ab-Labor: " Width="100" Foreground="Orange"/>
                                    <TextBlock x:Name="b_ab_max_lab_mep" Width="100" Foreground="Orange"/>
                                    <TextBlock x:Name="b_ab_max_lab_sys" Width="100" Foreground="Orange"/>
                                </DockPanel>
                                <Rectangle StrokeThickness="1" Stroke="Black" OpacityMask="Black"></Rectangle>
                                <DockPanel Margin="0,2,0,0" >
                                    <TextBlock Text="Ab-24h: " Width="100" Foreground="Orange"/>
                                    <TextBlock x:Name="b_ab_24h_max_mep" Width="100" Foreground="Orange"/>
                                    <TextBlock x:Name="b_ab_24h_max_sys" Width="100" Foreground="Orange"/>
                                </DockPanel>
                                <Rectangle StrokeThickness="2" Stroke="Black" OpacityMask="Black"></Rectangle>
                                <DockPanel Margin="0,10,0,0" >
                                    <TextBlock Text="Luftbilanz:" Width="100" FontWeight="Bold"  FontSize="14"/>
                                    <TextBlock x:Name="max_Abweichung_mep" Width="100"  FontWeight="Bold"  FontSize="14"/>
                                    <TextBlock x:Name="max_Abweichung_sys" Width="100"  FontWeight="Bold"  FontSize="14"/>
                                </DockPanel>
                                <Rectangle StrokeThickness="2" Stroke="Black" OpacityMask="Black"></Rectangle>
                                <DockPanel Margin="0,10,0,0" >
                                    <TextBlock Text="Druckstufe: " Width="100" FontWeight="Bold"  FontSize="14"/>
                                    <TextBlock x:Name="max_auswertung_druckstufe" Width="100"  FontWeight="Bold"  FontSize="14"/>
                                </DockPanel>
                                <Rectangle StrokeThickness="2" Stroke="Black" OpacityMask="Black"></Rectangle>
                                <DockPanel Margin="0,10,0,0" >
                                    <TextBlock Text="Auswertung: " Width="100" FontWeight="Bold"  FontSize="14"/>
