                                    <Trigger Property="IsSelected" Value="True">
                                        <Setter Property="Background" Value="#FFC3CCD4"/>
                                    </Trigger>
                                </Style.Triggers>

                            </Style>
                        </DataGrid.CellStyle>

                        <DataGrid.Columns>
                            <DataGridTemplateColumn Width="3*">
                                <DataGridTemplateColumn.CellTemplate>
                                    <DataTemplate>
                                        <TextBlock Text="{Binding Path = familyandtyp}" TextWrapping="Wrap"/>
                                    </DataTemplate>
                                </DataGridTemplateColumn.CellTemplate>
                                <DataGridTemplateColumn.Header>
                                    <TextBlock Text="Familien und Typ" HorizontalAlignment="Center" VerticalAlignment="Center" ToolTip="Familienname:Typenname"/>
                                </DataGridTemplateColumn.Header>
                            </DataGridTemplateColumn>
                            <DataGridTemplateColumn Width="*">
                                <DataGridTemplateColumn.CellTemplate>
                                    <DataTemplate>
                                        <TextBlock Text="{Binding Path = menge}" TextWrapping="Wrap" />
                                    </DataTemplate>
                                </DataGridTemplateColumn.CellTemplate>
                                <DataGridTemplateColumn.Header>
                                    <TextBlock Text="Mengen" HorizontalAlignment="Center" VerticalAlignment="Center" />
                                </DataGridTemplateColumn.Header>
                            </DataGridTemplateColumn>
                            <DataGridTemplateColumn Width="2*">
                                <DataGridTemplateColumn.CellTemplate>
                                    <DataTemplate>
                                        <TextBlock Text="{Binding Path = anderesraum}" TextWrapping="Wrap" />
                                    </DataTemplate>
                                </DataGridTemplateColumn.CellTemplate>
                                <DataGridTemplateColumn.Header>
                                    <TextBlock Text="aus dem Raum" HorizontalAlignment="Center" VerticalAlignment="Center" />
                                </DataGridTemplateColumn.Header>
                            </DataGridTemplateColumn>
                        </DataGrid.Columns>
                    </DataGrid>


                </GroupBox>

            </Grid>
        </GroupBox>
