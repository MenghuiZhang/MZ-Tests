<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MEP-Räume-Übersicht" Height="900" Width="1900" MinWidth="1700" Background="SkyBlue">
    <Grid Margin="10">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="450"/>
            <ColumnDefinition/>
            <ColumnDefinition Width="350"/>

        </Grid.ColumnDefinitions>

        <Grid.RowDefinitions>
            <RowDefinition Height="180"/>
            <RowDefinition Height="4*"/>
            <RowDefinition Height="*"/>

            <RowDefinition Height="60"/>

        </Grid.RowDefinitions>

        <GroupBox VerticalAlignment="Center" Margin="10,0,10,0" Grid.Column="0" Grid.ColumnSpan="2">
            <GroupBox.Header>
                <TextBlock Text="MEP-Raum" FontWeight="Bold"/>
            </GroupBox.Header>
            <Grid>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="1.5*"/>
                    <ColumnDefinition/>
                    <ColumnDefinition/>
                    <ColumnDefinition/>
                    <ColumnDefinition/>
                    <ColumnDefinition/>
                </Grid.ColumnDefinitions>
                <GroupBox Header="Raumfinder" Margin="5,5,5,5">
                    <StackPanel>
                        <DockPanel Height="20" VerticalAlignment="Center" Margin="5,5,5,5">
                            <TextBlock Text="Filter: " VerticalAlignment="Center" Margin="0,0,0,0" Foreground="#FFAE4141" FontWeight="Bold" ToolTip="Nummer" Width="40"/>
                            <TextBox GotFocus="TextBox_GotFocus" x:Name="suche" VerticalAlignment="Center" TextChanged="MEP_Suche_changed"  />
                        </DockPanel>
                        <DockPanel Height="20" VerticalAlignment="Center" Margin="5,5,5,5">
                            <TextBlock Text="Raum: " VerticalAlignment="Center" Margin="0,0,0,0" Foreground="#FFAE4141" FontWeight="Bold" ToolTip="Nummer" Width="40"/>
                            <ComboBox x:Name="Raumnr" IsEditable="True" VerticalAlignment="Center" SelectionChanged="MEP_changed"/>
                        </DockPanel>
                        <Button x:Name="rauman" Click="raumanzeigen" Content="Raum anzeigen" VerticalAlignment="Center" Margin="5,5,5,0" FontWeight="Bold" ToolTip="Raum anzeigen" IsEnabled="False"/>
                        <Button x:Name="rauman1" Click="bauteilundraumanzeigen" Content="Raum und alle Bauteile anzeigen" VerticalAlignment="Center" Margin="5,5,5,0" FontWeight="Bold" ToolTip="Raum anzeigen" IsEnabled="False"/>
                    </StackPanel>

                </GroupBox>
                <GroupBox Header="Grundinfo." Margin="5,5,5,5" Grid.Column="1">
                    <StackPanel>
                        <DockPanel Height="20" Margin="5,0,5,0">
                            <TextBlock Text="Ebene: " VerticalAlignment="Center" Margin="0,0,5,0" Foreground="#FFAE4141" FontWeight="Bold" ToolTip="Ebene" Width="42" />
                            <TextBlock x:Name="ebene" HorizontalAlignment="Left" VerticalAlignment="Center" Width="130"/>
                        </DockPanel>
                        <DockPanel Height="20" Margin="5,0,5,0">
                            <TextBlock VerticalAlignment="Center" Margin="0,0,5,0" Foreground="#FFAE4141" FontWeight="Bold" Text="Fläche: " Width="65"/>
                            <TextBlock x:Name="flaeche" VerticalAlignment="Center" Width="70" />
                            <TextBlock Text=" m²" VerticalAlignment="Center" Margin="0" />
                        </DockPanel>
                        <DockPanel Height="20" Margin="0,0,5,0">
                            <TextBlock VerticalAlignment="Center" Margin="5,0,5,0" Foreground="#FFAE4141" FontWeight="Bold" Text="Licht Höhe:" Width="65"/>
                            <TextBox x:Name="hoehe" VerticalAlignment="Center" Width="70" TextChanged="hoehechanged" PreviewTextInput="Textinput0"/>
                            <TextBlock Text=" mm" VerticalAlignment="Center" Margin="0" />
                        </DockPanel>
                        <DockPanel Height="20" Margin="5,0,5,0">
                            <TextBlock VerticalAlignment="Center" Margin="0,0,5,0" Foreground="#FFAE4141" FontWeight="Bold" Text="Volumen: " Width="65"/>
                            <TextBlock x:Name="volumen" VerticalAlignment="Center" Width="70" />
                            <TextBlock Text=" m³" VerticalAlignment="Center" Margin="0" />
                        </DockPanel>

                        <DockPanel Height="20" Margin="0,0,5,0">
                            <TextBlock VerticalAlignment="Center" Margin="5,0,5,0" Foreground="#FFAE4141" FontWeight="Bold" Text="Personen: " Width="70"/>
                            <TextBlock x:Name="personen" VerticalAlignment="Center" />
                        </DockPanel>

                    </StackPanel>
                </GroupBox>
                <GroupBox Header="Tagbetrieb" Margin="5,5,5,5" Grid.Column="2">
                    <StackPanel Margin="5,0,5,0">
                        <TextBlock Text="Berechnung nach: " VerticalAlignment="Center" Foreground="#FFAE4141" FontWeight="Bold" ToolTip="TGA_RLT_VolumenstromProName" Margin="0,5,0,0"/>
                        <ComboBox x:Name="bezugsname" IsEnabled="False" VerticalAlignment="Center" SelectionChanged="bezugsnameselectionchanged" Margin="0,5,0,0"/>
                        <TextBlock Text="Faktor:" VerticalAlignment="Center" Foreground="#FFAE4141" FontWeight="Bold" ToolTip="TGA_RLT_VolumenstromProFaktor" Margin="0,5,0,0"/>
                        <DockPanel>
                            <TextBox GotFocus="TextBox_GotFocus" PreviewTextInput="Textinput1" x:Name="faktor" IsEnabled="False" VerticalAlignment="Center" Margin="0,5,0,5" Width="50" HorizontalAlignment="Stretch" VerticalContentAlignment="Center" TextChanged="faktorchanged"/>
                            <TextBlock x:Name="einheit" VerticalAlignment="Center" />
                        </DockPanel>
                    </StackPanel>
                </GroupBox>
                <GroupBox Header="Nachtbetrieb" Margin="5,5,5,5" Grid.Column="3">
                    <StackPanel>
                        <DockPanel Margin="5,5,5,5">
                            <TextBlock Text="Nachtsbetrieb:" VerticalAlignment="Center" Foreground="#FFAE4141" FontWeight="Bold" ToolTip="IGF_RLT_Nachtbetrieb"/>
                            <CheckBox x:Name="isnachtbetrieb" IsEnabled="False" Checked="nachtbetriebchanged" Unchecked="nachtbetriebchanged" Margin="5,0,0,0" VerticalContentAlignment="Center"/>
                        </DockPanel>
                        <DockPanel Margin="5,5,5,5">
                            <TextBlock Text="Luftwechsel:" VerticalAlignment="Center" Foreground="#FFAE4141" FontWeight="Bold" ToolTip="IGF_RLT_NachtbetriebLW" Width="75" Height="18"/>
                            <TextBox GotFocus="TextBox_GotFocus" x:Name="LW_nacht" IsEnabled="False" Width="25" Height="18" TextChanged="LW_nacht_changed" PreviewTextInput="Textinput1"/>
                            <TextBlock Text="-1/h" Margin="5,0,0,0"/>
                        </DockPanel>
                        <DockPanel Margin="5,5,5,5">
                            <TextBlock Text="Von:" VerticalAlignment="Center" Foreground="#FFAE4141" FontWeight="Bold" ToolTip="IGF_RLT_NachtbetriebVon" Width="75" Height="18"/>
                            <TextBox GotFocus="TextBox_GotFocus" x:Name="von_nacht" Height="18" IsEnabled="False" TextChanged="von_nacht_changed" PreviewTextInput="Textinput1"/>
                        </DockPanel>
                        <DockPanel Margin="5,5,5,5">
                            <TextBlock Text="Bis:" VerticalAlignment="Center" Foreground="#FFAE4141" FontWeight="Bold" ToolTip="IGF_RLT_NachtbetriebBis" Width="75" Height="18"/>
                            <TextBox GotFocus="TextBox_GotFocus" x:Name="bis_nacht" Height="18" IsEnabled="False" TextChanged="bis_nacht_changed" PreviewTextInput="Textinput1"/>
                        </DockPanel>
                    </StackPanel>
                </GroupBox>
                <GroupBox Header="TiefeNachtbetrieb" Margin="5,5,5,5" Grid.Column="4">
                    <StackPanel>
                        <DockPanel Margin="5,5,5,5">
                            <TextBlock Text="T-Nachtsbetrieb:" VerticalAlignment="Center" Foreground="#FFAE4141" FontWeight="Bold" ToolTip="IGF_RLT_TieferNachtbetrieb"/>
                            <CheckBox x:Name="istiefenachtbetrieb" IsEnabled="False" Checked="tnachtbetriebchanged" Unchecked="tnachtbetriebchanged" Margin="5,0,0,0" VerticalContentAlignment="Center"/>
                        </DockPanel>
                        <DockPanel Margin="5,5,5,5">
                            <TextBlock Text="Luftwechsel:" VerticalAlignment="Center" Foreground="#FFAE4141" FontWeight="Bold" ToolTip="IGF_RLT_TieferNachtbetriebLW" Width="75" Height="18"/>
                            <TextBox GotFocus="TextBox_GotFocus" x:Name="LW_tnacht" IsEnabled="False" Width="25" Height="18" TextChanged="LW_tnacht_changed" PreviewTextInput="Textinput1"/>
                            <TextBlock Text="-1/h" Margin="5,0,0,0"/>
                        </DockPanel>
                        <DockPanel Margin="5,5,5,5">
                            <TextBlock Text="Von:" VerticalAlignment="Center" Foreground="#FFAE4141" FontWeight="Bold" ToolTip="IGF_RLT_TieferNachtbetriebVon" Width="75" Height="18"/>
                            <TextBox GotFocus="TextBox_GotFocus" x:Name="von_tnacht" Height="18" IsEnabled="False" TextChanged="von_tnacht_changed" PreviewTextInput="Textinput1"/>
                        </DockPanel>
                        <DockPanel Margin="5,5,5,5">
                            <TextBlock Text="Bis:" VerticalAlignment="Center" Foreground="#FFAE4141" FontWeight="Bold" ToolTip="IGF_RLT_TieferNachtbetriebBis" Width="75" Height="18"/>
                            <TextBox GotFocus="TextBox_GotFocus"  x:Name="bis_tnacht" Height="18" IsEnabled="False" TextChanged="bis_tnacht_changed" PreviewTextInput="Textinput1"/>
                        </DockPanel>
                    </StackPanel>
                </GroupBox>
                <GroupBox Header="Laboreingabe (m³/h)" Margin="5,5,5,5" Grid.Column="5">
                    <StackPanel>

                        <DockPanel Margin="5,5,5,5">
                            <TextBlock Text="LABmin:" VerticalAlignment="Center" Foreground="#FFAE4141" FontWeight="Bold" ToolTip="IGF_RLT_Nachtbetrieb" Width="75" Height="18"/>
                            <TextBox GotFocus="TextBox_GotFocus" x:Name="labmineingabe" IsEnabled="False" Width="70" Height="18" TextChanged="labmineingabe_changed" PreviewTextInput="Textinput0"/>

                        </DockPanel>
                        <DockPanel Margin="5,5,5,5">
                            <TextBlock Text="LABmax:" VerticalAlignment="Center" Foreground="#FFAE4141" FontWeight="Bold" ToolTip="IGF_RLT_Nachtbetrieb" Width="75" Height="18"/>
                            <TextBox GotFocus="TextBox_GotFocus" x:Name="labmaxeingabe" IsEnabled="False" Width="70" Height="18" TextChanged="labmaxeingabe_changed" PreviewTextInput="Textinput0"/>

                        </DockPanel>
                        <DockPanel Margin="5,5,5,5">
                            <TextBlock Text="24h-Ab:" VerticalAlignment="Center" Foreground="#FFAE4141" FontWeight="Bold" ToolTip="IGF_RLT_Nachtbetrieb" Width="75                                        " Height="18"/>
                            <TextBox GotFocus="TextBox_GotFocus" x:Name="ab24heingabe" IsEnabled="False" Width="70" Height="18" TextChanged="ab24heingabe_changed" PreviewTextInput="Textinput0"/>
                        </DockPanel>
                        <DockPanel Margin="5,5,5,5">
                            <TextBlock Text="Druckstufe:" VerticalAlignment="Center" Foreground="#FFAE4141" FontWeight="Bold" ToolTip="IGF_RLT_Nachtbetrieb" Width="75" Height="18"/>
                            <TextBox GotFocus="TextBox_GotFocus" x:Name="druckeingabe" IsEnabled="False" Width="70" Height="18" TextChanged="druckeingabe_changed" PreviewTextInput="Textinput0"/>
                        </DockPanel>
                    </StackPanel>
                </GroupBox>
            </Grid>



        </GroupBox>
        <StackPanel Grid.Column="5" x:Name="warnung" Margin="0,15,0,0">
            <TextBlock Text="Achtung: Der VSR versorgt mehrere Räume!" Foreground="#FFFF0505" TextWrapping="Wrap" VerticalAlignment="Top" Margin="10,0" FontSize="14"/>
            <TextBlock Text="-------------------------------------------------------------" VerticalAlignment="Top" Margin="10,0" FontSize="14"/>
            <TextBlock x:Name="Rauminfo" Text="Die angezeigte Rauminfomationen beziehen sich auf Raum =&gt;" TextWrapping="Wrap" Visibility="Visible" FontSize="14" Foreground="#FFF5085E" HorizontalAlignment="Stretch" VerticalAlignment="Center" Padding="10,0,0,0"/>
            <StackPanel Height="40"  Margin="10,0,10,10"  VerticalAlignment="Bottom">
                <TextBlock x:Name="raumwechsel_hinweis" Text="Hier den Raum wechseln:"/>
                <ComboBox x:Name="auswahl"  SelectionChanged="VSR_MEPRaum_changed"/>
            </StackPanel>
        </StackPanel>

        <DockPanel Margin="10,5,5,10" Grid.Row="1" >
            <TabControl  Background="LightBlue">
                <TabItem >
                    <TabItem.Header>
                        <TextBlock Text="Übersicht" FontWeight="Bold"/>
                    </TabItem.Header>
                    <DataGrid x:Name="grundinfo" Background="LightBlue" ItemsSource="{Binding}" CanUserSortColumns="False">
                        <DataGrid.CellStyle>
                            <Style TargetType="{x:Type DataGridCell}">
                                <Setter Property="ToolTip" Value="{Binding tooltip}"/>
                                <Style.Triggers>
                                    <DataTrigger Binding="{Binding name}" Value="24h Ab">
                                        <Setter Property="Foreground" Value="#FFC85B00"/>
                                    </DataTrigger>
                                    <DataTrigger Binding="{Binding name}" Value="min.RAB">
                                        <Setter Property="Foreground" Value="Orange"/>
                                    </DataTrigger>
                                    <DataTrigger Binding="{Binding name}" Value="min.AB_SUM">
                                        <Setter Property="Foreground" Value="Orange"/>
                                    </DataTrigger>
                                    <DataTrigger Binding="{Binding name}" Value="min.LAB">
                                        <Setter Property="Foreground" Value="orange"/>
                                    </DataTrigger>
                                    <DataTrigger Binding="{Binding name}" Value="min.RZU">
                                        <Setter Property="Foreground" Value="Blue"/>
                                    </DataTrigger>
                                    <DataTrigger Binding="{Binding name}" Value="min.ZU_SUM">
                                        <Setter Property="Foreground" Value="Blue"/>
                                    </DataTrigger>
                                    <DataTrigger Binding="{Binding name}" Value="max.RAB">
                                        <Setter Property="Foreground" Value="Orange"/>
                                    </DataTrigger>
                                    <DataTrigger Binding="{Binding name}" Value="max.AB_SUM">
                                        <Setter Property="Foreground" Value="Orange"/>
                                    </DataTrigger>
                                    <DataTrigger Binding="{Binding name}" Value="max.LAB">
                                        <Setter Property="Foreground" Value="Orange"/>
                                    </DataTrigger>
                                    <DataTrigger Binding="{Binding name}" Value="max.RZU">
                                        <Setter Property="Foreground" Value="Blue"/>
                                    </DataTrigger>
                                    <DataTrigger Binding="{Binding name}" Value="max.ZU_SUM">
                                        <Setter Property="Foreground" Value="Blue"/>
                                    </DataTrigger>
                                    <DataTrigger Binding="{Binding name}" Value="NB Zu">
                                        <Setter Property="Foreground" Value="Blue"/>
                                    </DataTrigger>
                                    <DataTrigger Binding="{Binding name}" Value="TNB Zu">
                                        <Setter Property="Foreground" Value="Blue"/>
                                    </DataTrigger>
                                    <DataTrigger Binding="{Binding name}" Value="NB Ab">
                                        <Setter Property="Foreground" Value="Orange"/>
                                    </DataTrigger>
                                    <DataTrigger Binding="{Binding name}" Value="TNB Ab">
                                        <Setter Property="Foreground" Value="Orange"/>
                                    </DataTrigger>



                                    <DataTrigger Binding="{Binding name}" Value="min.TZU">
                                        <Setter Property="Foreground" Value="Blue"/>
                                    </DataTrigger>
                                    <DataTrigger Binding="{Binding name}" Value="max.TZU">
                                        <Setter Property="Foreground" Value="Blue"/>
                                    </DataTrigger>
                                    <DataTrigger Binding="{Binding name}" Value="max.TAB">
                                        <Setter Property="Foreground" Value="Orange"/>
                                    </DataTrigger>
                                    <DataTrigger Binding="{Binding name}" Value="min.TAB">
                                        <Setter Property="Foreground" Value="Orange"/>
                                    </DataTrigger>
                                    <DataTrigger Binding="{Binding name}" Value="Überstrom">
                                        <Setter Property="Foreground" Value="DarkGreen"/>
                                    </DataTrigger>
                                    <DataTrigger Binding="{Binding name}" Value="Über. Ein.">
                                        <Setter Property="Foreground" Value="DarkGreen"/>
                                    </DataTrigger>
                                    <DataTrigger Binding="{Binding name}" Value="Über. Aus.">
                                        <Setter Property="Foreground" Value="DarkGreen"/>
                                    </DataTrigger>
                                    <DataTrigger Binding="{Binding name}" Value="Über.Ein.M.">
                                        <Setter Property="Foreground" Value="DarkGreen"/>
                                    </DataTrigger>
                                    <DataTrigger Binding="{Binding name}" Value="Über.Aus.M.">
                                        <Setter Property="Foreground" Value="DarkGreen"/>
                                    </DataTrigger>
                                </Style.Triggers>
                            </Style>
                        </DataGrid.CellStyle>
                        <DataGrid.Columns>
                            <DataGridTextColumn IsReadOnly="True" Header="Art" Width="*" Binding="{Binding name}"/>
                            <DataGridTextColumn IsReadOnly="True" Header="Soll (m³/h)" Width="*" Binding="{Binding Path= soll}"/>
                            <DataGridTextColumn IsReadOnly="True" Header="Ist (m³/h)" Width="*" Binding="{Binding Path= ist}"/>
                        </DataGrid.Columns>
                    </DataGrid>
                </TabItem>
                <TabItem >
                    <TabItem.Header>
                        <TextBlock Text="Detailsansicht" FontWeight="Bold"/>
                    </TabItem.Header>
                    <TabControl  Background="LightBlue">
                        <TabItem Header="TagMinbetrieb"  Width="100">
                            <StackPanel Background="White" Margin="0,0,0,0">
                                <DockPanel Margin="0,5,0,0" >
                                    <TextBlock Text="" FontWeight="Bold" Width="100" Foreground="Blue"/>
                                    <TextBlock  FontWeight="Bold" Text="MEP" Width="100" />
                                    <TextBlock FontWeight="Bold" Text="Anlagen" Width="100" />
                                </DockPanel>
                                <Rectangle StrokeThickness="2" Stroke="Black" OpacityMask="Black"/>
                                <DockPanel Margin="0,5,0,0" >
                                    <TextBlock Text="Zuluft-Summe: " FontWeight="Bold" Width="100" Foreground="Blue"/>
                                    <TextBlock x:Name="b_zu_min_sum_mep" FontWeight="Bold" Width="100" Foreground="Blue"/>
                                    <TextBlock x:Name="b_zu_min_sum_sys" FontWeight="Bold" Width="100" Foreground="Blue"/>
                                </DockPanel>
                                <Rectangle StrokeThickness="1" Stroke="Black" OpacityMask="Black"/>
                                <DockPanel Margin="0,5,0,0" >
                                    <TextBlock Text="Zu-Raum: " Width="100"  Foreground="Blue"/>
                                    <TextBlock x:Name="b_zu_min_raum_mep" Width="100" Foreground="Blue"/>
                                    <TextBlock x:Name="b_zu_min_raum_sys" Width="100"   Foreground="Blue"/>
                                </DockPanel>

                                <Rectangle StrokeThickness="1" Stroke="Black" OpacityMask="Black"/>
                                <DockPanel Margin="0,2,0,0" >
                                    <TextBlock Text="Zu-Überstrom: " Width="100"  Foreground="Blue"/>
                                    <TextBlock x:Name="b_zu_min_ueber_mep" Width="100" Foreground="Blue"/>
                                    <TextBlock x:Name="b_zu_min_ueber_sys" Width="100" Foreground="Blue"/>
                                </DockPanel>
                                <Rectangle StrokeThickness="2" Stroke="Black" OpacityMask="Black"/>
                                <DockPanel Margin="0,2,0,0" >
                                    <TextBlock Text="Zu-Überstrom_M: " Width="100"  Foreground="Blue"/>
                                    <TextBlock x:Name="b_zu_min_ueber_m_mep" Width="100" Foreground="Blue"/>
                                    <TextBlock x:Name="b_zu_min_ueber_m_sys" Width="100" Foreground="Blue"/>
                                </DockPanel>
                                <Rectangle StrokeThickness="2" Stroke="Black" OpacityMask="Black"/>

                                <DockPanel Margin="0,10,0,0" >
                                    <TextBlock Text="Abluft-Summe: " FontWeight="Bold" Width="100"  Foreground="Orange"/>
                                    <TextBlock x:Name="b_ab_min_sum_mep" FontWeight="Bold" Width="100" Foreground="Orange"/>
                                    <TextBlock x:Name="b_ab_min_sum_sys" FontWeight="Bold" Width="100" Foreground="Orange"/>
                                </DockPanel>
                                <Rectangle StrokeThickness="1" Stroke="Black" OpacityMask="Black"/>
                                <DockPanel Margin="0,5,0,0" >
                                    <TextBlock Text="Ab-Raum: " Width="100" Foreground="Orange"/>
                                    <TextBlock x:Name="b_ab_min_raum_mep" Width="100"  Foreground="Orange"/>
                                    <TextBlock x:Name="b_ab_min_raum_sys" Width="100"  Foreground="Orange"/>
                                </DockPanel>

                                <Rectangle StrokeThickness="1" Stroke="Black" OpacityMask="Black"/>
                                <DockPanel Margin="0,2,0,0" >
                                    <TextBlock Text="Ab-Überstrom: " Width="100" Foreground="Orange"/>
                                    <TextBlock x:Name="b_ab_min_ueber_mep" Width="100" Foreground="Orange"/>
                                    <TextBlock x:Name="b_ab_min_ueber_sys" Width="100" Foreground="Orange"/>
                                </DockPanel>
                                <Rectangle StrokeThickness="1" Stroke="Black" OpacityMask="Black"/>
                                <DockPanel Margin="0,2,0,0" >
                                    <TextBlock Text="Ab-Überstrom_M: " Width="100" Foreground="Orange"/>
                                    <TextBlock x:Name="b_ab_min_ueber_m_mep" Width="100" Foreground="Orange"/>
                                    <TextBlock x:Name="b_ab_min_ueber_m_sys" Width="100" Foreground="Orange"/>
                                </DockPanel>
                                <Rectangle StrokeThickness="1" Stroke="Black" OpacityMask="Black"/>
                                <DockPanel Margin="0,2,0,0" >
                                    <TextBlock Text="Ab-Labor: " Width="100" Foreground="Orange"/>
                                    <TextBlock x:Name="b_ab_min_lab_mep" Width="100" Foreground="Orange"/>
                                    <TextBlock x:Name="b_ab_min_lab_sys" Width="100" Foreground="Orange"/>
                                </DockPanel>
                                <Rectangle StrokeThickness="1" Stroke="Black" OpacityMask="Black"/>
                                <DockPanel Margin="0,2,0,0" >
                                    <TextBlock Text="Ab-24h: " Width="100" Foreground="Orange"/>
                                    <TextBlock x:Name="b_ab_24h_min_mep" Width="100" Foreground="Orange"/>
                                    <TextBlock x:Name="b_ab_24h_min_sys" Width="100" Foreground="Orange"/>
                                </DockPanel>
                                <Rectangle StrokeThickness="2" Stroke="Black" OpacityMask="Black"/>
                                <DockPanel Margin="0,10,0,0" >
                                    <TextBlock Text="Luftbilanz:" Width="100" FontWeight="Bold"  FontSize="14"/>
                                    <TextBlock x:Name="min_Abweichung_mep" Width="100"  FontWeight="Bold"  FontSize="14"/>
                                    <TextBlock x:Name="min_Abweichung_sys" Width="100"  FontWeight="Bold"  FontSize="14"/>
                                </DockPanel>
                                <Rectangle StrokeThickness="2" Stroke="Black" OpacityMask="Black"/>
                                <DockPanel Margin="0,10,0,0" >
                                    <TextBlock Text="Druckstufe: " Width="100" FontWeight="Bold"  FontSize="14"/>
                                    <TextBlock x:Name="min_auswertung_druckstufe" Width="100"  FontWeight="Bold"  FontSize="14"/>
                                </DockPanel>
                                <Rectangle StrokeThickness="2" Stroke="Black" OpacityMask="Black"/>
                                <DockPanel Margin="0,10,0,0" >
                                    <TextBlock Text="Auswertung: " Width="100" FontWeight="Bold"  FontSize="14"/>
                                    <TextBlock x:Name="min_auswertung_mep" Width="100"  FontWeight="Bold"  FontSize="14"/>
                                    <TextBlock x:Name="min_auswertung_sys" Width="100"  FontWeight="Bold"  FontSize="14"/>
                                </DockPanel>
                                <Rectangle StrokeThickness="2" Stroke="Black" OpacityMask="Black" Margin="0,0,0,0"/>
                                <Rectangle StrokeThickness="2" Stroke="Black" OpacityMask="Black" Margin="0,25,0,0"/>
                                <DockPanel Margin="0,0,0,0" >
                                    <TextBlock Text="Tierhaltung: " Width="100" FontWeight="Bold"  FontSize="14" HorizontalAlignment="Left"/>
                                </DockPanel>
                                <Rectangle StrokeThickness="1" Stroke="Black" OpacityMask="Black" Margin="0,0,0,0"/>
                                <DockPanel Margin="0,2,0,0" >
                                    <TextBlock Text="Zu: " Width="100"  Foreground="Blue"/>
                                    <TextBlock x:Name="b_zu_min_tier_mep" Width="100"  Foreground="Blue"/>
                                    <TextBlock x:Name="b_zu_min_tier_sys" Width="100" Foreground="Blue"/>
                                </DockPanel>
                                <Rectangle StrokeThickness="1" Stroke="Black" OpacityMask="Black"/>
                                <DockPanel Margin="0,2,0,0" >
                                    <TextBlock Text="Ab: " Width="100"  Foreground="Orange"/>
                                    <TextBlock x:Name="b_ab_min_tier_mep" Width="100" Foreground="Orange"/>
                                    <TextBlock x:Name="b_ab_min_tier_sys" Width="100" Foreground="Orange"/>
                                </DockPanel>
                                <Rectangle StrokeThickness="1" Stroke="Black" OpacityMask="Black"/>

                            </StackPanel>




                        </TabItem>
                        <TabItem Header="TagMaxbetrieb"  Width="100">
                            <StackPanel Background="White" Margin="0,0,0,0">
                                <DockPanel Margin="0,5,0,0" >
                                    <TextBlock Text="" FontWeight="Bold" Width="100" Foreground="Blue"/>
                                    <TextBlock  FontWeight="Bold" Text="MEP" Width="100" />
                                    <TextBlock FontWeight="Bold" Text="Anlagen" Width="100" />
                                </DockPanel>
                                <Rectangle StrokeThickness="2" Stroke="Black" OpacityMask="Black"/>
                                <DockPanel Margin="0,5,0,0" >
