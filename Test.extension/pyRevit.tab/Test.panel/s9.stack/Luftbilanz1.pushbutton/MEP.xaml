                                            <TextBlock Text="{Binding Path = anderesraum}" TextWrapping="Wrap" />
                                        </DataTemplate>
                                    </GridViewColumn.CellTemplate>
                                    <TextBlock Text="in Raum" HorizontalAlignment="Center" VerticalAlignment="Center" />
                                </GridViewColumn>
                            </GridView>
                        </ListView.View>

                    </ListView>
                </GroupBox>
                <GroupBox Header="Einströmung" Margin="2" Grid.Column="1">
                    <ListView x:Name="lv_ueber_in" Background="#FFC7EAF5" ItemsSource="{Binding}" SelectionMode="Single" SelectionChanged="Ueber_in_Selected_Changed">
                        <ListView.Resources>
                            <Style TargetType="{x:Type GridViewColumnHeader}">
                                <Setter Property="FontWeight" Value="Bold"/>
                                <Setter Property="FontSize" Value="10"/>
                                <Setter Property="Height" Value="18"/>
                            </Style>
                        </ListView.Resources>
                        <ListView.ItemContainerStyle>
                            <Style TargetType="ListViewItem">
                                <Setter Property="Foreground" Value="DarkGreen"/>
                            </Style>
                        </ListView.ItemContainerStyle>


                        <ListView.View>
                            <GridView>

                                <GridViewColumn Width="300" >
                                    <GridViewColumn.CellTemplate>
                                        <DataTemplate>
                                            <TextBlock Text="{Binding Path = familyandtyp}" TextWrapping="Wrap"/>
                                        </DataTemplate>
                                    </GridViewColumn.CellTemplate>
                                    <TextBlock Text="Familien und Typ" HorizontalAlignment="Center" VerticalAlignment="Center" ToolTip="Familienname:Typenname"/>
                                </GridViewColumn>
                                <GridViewColumn Width="60" >
                                    <GridViewColumn.CellTemplate>
                                        <DataTemplate>
                                            <TextBlock Text="{Binding Path = menge}" TextWrapping="Wrap"/>
                                        </DataTemplate>
                                    </GridViewColumn.CellTemplate>
                                    <TextBlock Text="Mengen" HorizontalAlignment="Center" VerticalAlignment="Center" />
                                </GridViewColumn>
                                <GridViewColumn Width="200" >
                                    <GridViewColumn.CellTemplate>
                                        <DataTemplate>
                                            <TextBlock Text="{Binding Path = anderesraum}" TextWrapping="Wrap"/>
                                        </DataTemplate>
                                    </GridViewColumn.CellTemplate>
                                    <TextBlock Text="aus Raum" HorizontalAlignment="Center" VerticalAlignment="Center" />
                                </GridViewColumn>
                            </GridView>
                        </ListView.View>

                    </ListView>
                </GroupBox>

            </Grid>
        </GroupBox>

        <Grid Grid.Row="3" Grid.ColumnSpan="3">
            <Grid.ColumnDefinitions>
                <ColumnDefinition/>
                <ColumnDefinition/>
                <ColumnDefinition/>
                <ColumnDefinition/>
                <ColumnDefinition/>
                <ColumnDefinition/>
                <ColumnDefinition/>
                <ColumnDefinition/>
                <ColumnDefinition/>
            </Grid.ColumnDefinitions>
            <Button Content="Raumluftbilanz berechnen" Width="200" Height="50" Grid.Column="0" Click="Berechnen" ToolTip="Luftmengeberechnen aller Räumen unf die Werte in Revit schreiben"/>

            <Button Width="200"   Height="50" Grid.Column="1" Click="luftmenge_gleich_verteilen_mep" ToolTip="Luftmengeberechnen in Auslässe verteilen und in Revit schreiben, nur für aktuellen Raum" >
                <Button.Content>
                    <StackPanel>
                        <TextBlock Text="Luftmenge in akt. Raum" HorizontalAlignment="Center"/>
                        <TextBlock Text="gleichmäßig verteilen" HorizontalAlignment="Center" />
                    </StackPanel>

                </Button.Content>
            </Button>

            <Button Width="200" Height="50" Grid.Column="2" Click="luftmenge_gleich_verteilen_pro" ToolTip="Luftmengeberechnen in Auslässe verteilen und in Revit schreiben, für alle Räume" >
                <Button.Content>
                    <StackPanel>
                        <TextBlock Text="Luftmenge für das Projekt" HorizontalAlignment="Center"/>
                        <TextBlock Text="gleichmäßig verteilen" HorizontalAlignment="Center" />
                    </StackPanel>

                </Button.Content>
            </Button>
            <Button Content="Änderung akt. Raum übernehmen" Width="200"   Height="50" Grid.Column="3" Click="aenderung_uebernehmen_mep" ToolTip="manuelle Eingabe in Revit schreiben"/>
            <Button Content="alle Änderung übernehmen" Width="200"   Height="50" Grid.Column="4" Click="aenderung_uebernehmen_pro" ToolTip="manuelle Eingabe in Revit schreiben"/>
            <Button  Width="200"   Height="50" Grid.Column="5" Click="aus_revit_uebernehmen" ToolTip="geänderte Daten wie z.B. Auslässesinfo in GUI übertragen und Übersicht aktualisieren">
                <Button.Content>
                    <StackPanel>
                        <TextBlock Text="Luftmenge für akt. Raum" HorizontalAlignment="Center"/>
                        <TextBlock Text="aus Modell übernehmen" HorizontalAlignment="Center" />
                    </StackPanel>
                </Button.Content>
            </Button>
            <Button Content="Raumluftbilanz exportieren" Width="200"  Height="50" Grid.Column="6" Click="exportieren" Visibility="Visible"/>
            <Button Content="VSR-Typ anpassen" Width="200"  Height="50" Grid.Column="7" Click="changevsrtype" Visibility="Visible"/>
            <Button Content="Programm schließen" Width="200"  Height="50" Grid.Column="8" Click="abbrechen" Visibility="Visible"/>
            <Grid Grid.Column="2" Visibility="Hidden">
                <Grid.RowDefinitions>
                    <RowDefinition/>
                    <RowDefinition/>
                    <RowDefinition/>
                </Grid.RowDefinitions>
                <Button Content="Büro" Click="buero" Height="19"/>
                <Button Content="Labor 1" Click="lab1" Grid.Row="1" Height="19"/>
                <Button Content="Labor 2" Click="lab2" Grid.Row="2" Height="19" HorizontalAlignment="Left" Width="100"/>
                <Button Content="Labor 3" Click="lab3" Grid.Row="2" Height="19" HorizontalAlignment="Right" Width="99.2"/>
            </Grid>
        </Grid>



    </Grid>
</Window>
