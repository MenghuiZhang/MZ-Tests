                    <DataGrid x:Name="anlageninfo" Background="LightBlue" ItemsSource="{Binding}" CanUserSortColumns="False">
                        <DataGrid.CellStyle>
                            <Style TargetType="{x:Type DataGridCell}">
                                <Style.Triggers>
                                    <DataTrigger Binding="{Binding name}" Value="RZU">
                                        <Setter Property="Foreground" Value="Blue"/>
                                    </DataTrigger>
                                    <DataTrigger Binding="{Binding name}" Value="TZU">
                                        <Setter Property="Foreground" Value="Blue"/>
                                    </DataTrigger>
                                    <DataTrigger Binding="{Binding name}" Value="RAB">
                                        <Setter Property="Foreground" Value="Orange"/>
                                    </DataTrigger>
                                    <DataTrigger Binding="{Binding name}" Value="TAB">
                                        <Setter Property="Foreground" Value="Orange"/>
                                    </DataTrigger>
                                    <DataTrigger Binding="{Binding name}" Value="24h">
                                        <Setter Property="Foreground" Value="#FFC85B00"/>
                                    </DataTrigger>
                                    <DataTrigger Binding="{Binding name}" Value="LAB">
                                        <Setter Property="Foreground" Value="orange"/>
