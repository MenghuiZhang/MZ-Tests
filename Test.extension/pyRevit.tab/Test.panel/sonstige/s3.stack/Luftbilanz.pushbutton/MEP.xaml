<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MEP-Räume-Übersicht" Height="900" Width="1800" MinWidth="1700" Background="SkyBlue">
    <Grid Margin="10">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="500"/>
            <ColumnDefinition/>
            <ColumnDefinition Width="350"/>
        </Grid.ColumnDefinitions>

        <Grid.RowDefinitions>
            <RowDefinition Height="0"/>
            <RowDefinition Height="190"/>
            <RowDefinition Height="479*"/>
            <RowDefinition Height="120*"/>
            <RowDefinition Height="60"/>

        </Grid.RowDefinitions>
        <Grid Grid.ColumnSpan="3" Margin="0,0,0,205" Grid.RowSpan="2">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="130"/>
                <ColumnDefinition/>
                <ColumnDefinition Width="100"/>

                <ColumnDefinition />
            </Grid.ColumnDefinitions>
        </Grid>

        <GroupBox VerticalAlignment="Top" Margin="10,0,10,0" Grid.Column="0" Grid.ColumnSpan="2" Grid.Row="1" Height="166">
            <GroupBox.Header>
                <TextBlock Text="MEP-Raum" FontWeight="Bold"/>
            </GroupBox.Header>
            <Grid>

                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="1.5*"/>
                    <ColumnDefinition/>
                    <ColumnDefinition Width="1.3*"/>
                    <ColumnDefinition/>
                    <ColumnDefinition/>
                    <ColumnDefinition/>
                </Grid.ColumnDefinitions>
                <GroupBox Header="Raumfinder" Margin="5,2,5,2">
                    <Grid>
                        <Grid.RowDefinitions>
                            <RowDefinition/>
                            <RowDefinition/>
                            <RowDefinition/>
                            <RowDefinition/>
                        </Grid.RowDefinitions>
                        <DockPanel Height="20" VerticalAlignment="Center" Margin="5,5,5,5">
                            <TextBlock Text="Filter: " VerticalAlignment="Center" Margin="0,0,0,0" Foreground="#FFAE4141" FontWeight="Bold" ToolTip="Nummer" Width="40"/>
                            <TextBox x:Name="suche" VerticalAlignment="Center" TextChanged="MEP_Suche_changed"  />
                        </DockPanel>
                        <DockPanel Height="20" VerticalAlignment="Center" Margin="5,5,5,5" Grid.Row="1">
                            <TextBlock Text="Raum: " VerticalAlignment="Center" Margin="0,0,0,0" Foreground="#FFAE4141" FontWeight="Bold" ToolTip="Nummer" Width="40"/>
                            <ComboBox x:Name="Raumnr" IsEditable="True" VerticalAlignment="Center" SelectionChanged="MEP_changed"/>
                        </DockPanel>
                        <Button x:Name="rauman" Click="raumanzeigen" Content="Raum anzeigen" VerticalAlignment="Center" Margin="5,5,5,0" FontWeight="Bold" ToolTip="Raum anzeigen" IsEnabled="False" Grid.Row="2"/>
                        <Button x:Name="rauman1" Click="bauteilundraumanzeigen" Content="Raum und alle Bauteile anzeigen" VerticalAlignment="Center" Margin="5,5,5,0" FontWeight="Bold" ToolTip="Raum anzeigen" IsEnabled="False" Grid.Row="3"/>
                    </Grid>

                </GroupBox>
                <GroupBox Header="Grundinfo." Margin="5,2,5,2" Grid.Column="1">
                    <Grid>
                        <Grid.RowDefinitions>
                            <RowDefinition/>
                            <RowDefinition/>
                            <RowDefinition/>
                            <RowDefinition/>
                            <RowDefinition/>
                        </Grid.RowDefinitions>
                        <DockPanel Height="20" Margin="5,0,0,0">
                            <TextBlock Text="Ebene: " VerticalAlignment="Center" Margin="0,0,5,0" Foreground="#FFAE4141" FontWeight="Bold" ToolTip="Ebene" Width="42" />
                            <TextBlock x:Name="ebene" HorizontalAlignment="Stretch" VerticalAlignment="Center"/>
                        </DockPanel>
                        <DockPanel Height="20" Margin="5,0,5,0" Grid.Row="1">
                            <TextBlock VerticalAlignment="Center" Margin="0,0,5,0" Foreground="#FFAE4141" FontWeight="Bold" Text="Fläche: " Width="65"/>
                            <TextBlock x:Name="flaeche" VerticalAlignment="Center" Width="70" />
                            <TextBlock Text=" m²" VerticalAlignment="Center" Margin="0" />
                        </DockPanel>
                        <DockPanel Height="20" Margin="5,0,5,0" Grid.Row="2">
                            <TextBlock VerticalAlignment="Center" Margin="0,0,5,0" Foreground="#FFAE4141" FontWeight="Bold" Text="Volumen: " Width="65"/>
                            <TextBlock x:Name="volumen" VerticalAlignment="Center" Width="70" />
                            <TextBlock Text=" m³" VerticalAlignment="Center" Margin="0" />
                        </DockPanel>
                        <DockPanel Height="20" Margin="0,0,5,0" Grid.Row="3">
                            <TextBlock VerticalAlignment="Center" Margin="5,0,5,0" Foreground="#FFAE4141" FontWeight="Bold" Text="Licht Höhe:" Width="65"/>
                            <TextBlock x:Name="hoehe" VerticalAlignment="Center" Width="70"/>
                            <TextBlock Text=" mm" VerticalAlignment="Center" Margin="0" />
                        </DockPanel>
                        <DockPanel Height="20" Margin="0,0,5,0" Grid.Row="4">
                            <TextBlock VerticalAlignment="Center" Margin="5,0,5,0" Foreground="#FFAE4141" FontWeight="Bold" Text="Personen: " Width="70"/>
                            <TextBlock x:Name="personen" VerticalAlignment="Center" />
                        </DockPanel>

                    </Grid>
                </GroupBox>
                <GroupBox Header="Tagbetrieb" Margin="5,2,5,2" Grid.Column="2">
                    <Grid>
                        <Grid.RowDefinitions>
                            <RowDefinition/>
                            <RowDefinition/>
                            <RowDefinition/>
                            <RowDefinition/>
                            <RowDefinition/>
                        </Grid.RowDefinitions>
                        <DockPanel>
                            <TextBlock Text="Berechnung nach: " VerticalAlignment="Center" Foreground="#FFAE4141" FontWeight="Bold" ToolTip="TGA_RLT_VolumenstromProName" Height="15"/>
                            <ComboBox x:Name="bezugsname" IsEnabled="False" VerticalAlignment="Center" SelectionChanged="bezugsnameselectionchanged"/>
                        </DockPanel>
                        <Grid Grid.Row="1">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition Width="1.5*"/>
                            </Grid.ColumnDefinitions>
                            <TextBlock Text="Faktor: " VerticalAlignment="Center" Foreground="#FFAE4141" FontWeight="Bold" ToolTip="TGA_RLT_VolumenstromProFaktor"/>
                            <TextBox PreviewTextInput="Textinput1" x:Name="faktor" IsEnabled="False" VerticalAlignment="Center" Margin="0,0,0,0" HorizontalAlignment="Stretch" VerticalContentAlignment="Center" TextChanged="faktorchanged" Grid.Column="1" Height="18"/>
                            <TextBlock x:Name="einheit" VerticalAlignment="Center" Grid.Column="2" Margin="10,0,0,0" />
                        </Grid>
                        <DockPanel Grid.Row="2">
                            <TextBlock Text="istReduziert:" VerticalAlignment="Center" Foreground="#FFAE4141" FontWeight="Bold" ToolTip="IGF_RLT_istReduziert" Width="140"/>
                            <CheckBox x:Name="istReduziert" IsEnabled="False" Checked="reduziertchanged" Unchecked="reduziertchanged" VerticalContentAlignment="Center" VerticalAlignment="Center"/>
                        </DockPanel>
                        <DockPanel Grid.Row="3">
                            <TextBlock Text="Raum-ReduziertFaktor:  " VerticalAlignment="Center" Foreground="#FFAE4141" FontWeight="Bold" ToolTip="IGF_RLT_Raum-ReduziertFaktor" Width="140"/>
                            <TextBox TextChanged="reduziertfaktorchanged" PreviewTextInput="Textinput1" x:Name="redufaktor" IsEnabled="False" VerticalAlignment="Center" Margin="0,0,0,0" HorizontalAlignment="Stretch" VerticalContentAlignment="Center" Grid.Column="1" Height="18"/>
                        </DockPanel>
                        <DockPanel Grid.Row="4">
                            <TextBlock Text="Druckstufe (m³/h):" VerticalAlignment="Center" Foreground="#FFAE4141" FontWeight="Bold" ToolTip="IGF_RLT_Nachtbetrieb" Width="140" Height="18"/>
                            <TextBox x:Name="druckeingabe" IsEnabled="False" Height="18" TextChanged="druckeingabe_changed" PreviewTextInput="Textinput2"/>
                        </DockPanel>
                    </Grid>
                </GroupBox>
                <GroupBox Header="Nachtbetrieb" Margin="5,2,5,2" Grid.Column="3">
                    <Grid>
                        <Grid.RowDefinitions>
                            <RowDefinition/>
                            <RowDefinition/>
                            <RowDefinition/>
                            <RowDefinition/>
                        </Grid.RowDefinitions>
                        <DockPanel Margin="5,0,5,0">
                            <TextBlock Text="Nachtsbetrieb:" VerticalAlignment="Center" Foreground="#FFAE4141" FontWeight="Bold" ToolTip="IGF_RLT_Nachtbetrieb"/>
                            <CheckBox x:Name="isnachtbetrieb" IsEnabled="False" Checked="nachtbetriebchanged" Unchecked="nachtbetriebchanged" Margin="5,0,0,0" VerticalContentAlignment="Center" VerticalAlignment="Center"/>
                        </DockPanel>
                        <DockPanel Margin="5,0,5,0" Grid.Row="1">
                            <TextBlock Text="Luftwechsel:" VerticalAlignment="Center" Foreground="#FFAE4141" FontWeight="Bold" ToolTip="IGF_RLT_NachtbetriebLW" Width="75" Height="18"/>
                            <TextBox x:Name="LW_nacht" IsEnabled="False" Width="40" Height="18" TextChanged="LW_nacht_changed" PreviewTextInput="Textinput1"/>
                            <TextBlock Text="-1/h" Margin="5,0,0,0" VerticalAlignment="Center"/>
                        </DockPanel>
                        <DockPanel Margin="5,0,5,0" Grid.Row="2">
                            <TextBlock Text="Von:" VerticalAlignment="Center" Foreground="#FFAE4141" FontWeight="Bold" ToolTip="IGF_RLT_NachtbetriebVon" Width="75" Height="18"/>
                            <TextBox x:Name="von_nacht" Height="18" IsEnabled="False" TextChanged="von_nacht_changed" PreviewTextInput="Textinput1"/>
                        </DockPanel>
                        <DockPanel Margin="5,0,5,0" Grid.Row="3">
                            <TextBlock Text="Bis:" VerticalAlignment="Center" Foreground="#FFAE4141" FontWeight="Bold" ToolTip="IGF_RLT_NachtbetriebBis" Width="75" Height="18"/>
                            <TextBox x:Name="bis_nacht" Height="18" IsEnabled="False" TextChanged="bis_nacht_changed" PreviewTextInput="Textinput1"/>
                        </DockPanel>
                    </Grid>
                </GroupBox>
                <GroupBox Header="TiefeNachtbetrieb" Margin="5,2,5,2" Grid.Column="4">
                    <Grid>
                        <Grid.RowDefinitions>
                            <RowDefinition/>
                            <RowDefinition/>
                            <RowDefinition/>
                            <RowDefinition/>
                        </Grid.RowDefinitions>
                        <DockPanel Margin="5,0,5,0">
                            <TextBlock Text="T-Nachtsbetrieb:" VerticalAlignment="Center" Foreground="#FFAE4141" FontWeight="Bold" ToolTip="IGF_RLT_TieferNachtbetrieb"/>
                            <CheckBox x:Name="istiefenachtbetrieb" IsEnabled="False" Checked="tnachtbetriebchanged" Unchecked="tnachtbetriebchanged" Margin="5,0,0,0" VerticalContentAlignment="Center" VerticalAlignment="Center"/>
                        </DockPanel>
                        <DockPanel Margin="5,0,5,0" Grid.Row="1">
                            <TextBlock Text="Luftwechsel:" VerticalAlignment="Center" Foreground="#FFAE4141" FontWeight="Bold" ToolTip="IGF_RLT_TieferNachtbetriebLW" Width="75" Height="18"/>
                            <TextBox x:Name="LW_tnacht" IsEnabled="False" Width="40" Height="18" TextChanged="LW_tnacht_changed" PreviewTextInput="Textinput1"/>
                            <TextBlock Text="-1/h" Margin="5,0,0,0" VerticalAlignment="Center"/>
                        </DockPanel>
                        <DockPanel Margin="5,0,5,0" Grid.Row="2">
                            <TextBlock Text="Von:" VerticalAlignment="Center" Foreground="#FFAE4141" FontWeight="Bold" ToolTip="IGF_RLT_TieferNachtbetriebVon" Width="75" Height="18"/>
                            <TextBox x:Name="von_tnacht" Height="18" IsEnabled="False" TextChanged="von_tnacht_changed" PreviewTextInput="Textinput1"/>
                        </DockPanel>
                        <DockPanel Margin="5,0,5,0" Grid.Row="3">
                            <TextBlock Text="Bis:" VerticalAlignment="Center" Foreground="#FFAE4141" FontWeight="Bold" ToolTip="IGF_RLT_TieferNachtbetriebBis" Width="75" Height="18"/>
                            <TextBox x:Name="bis_tnacht" Height="18" IsEnabled="False" TextChanged="bis_tnacht_changed" PreviewTextInput="Textinput1"/>
                        </DockPanel>
                    </Grid>
                </GroupBox>
                <GroupBox Header="Laboreingabe (m³/h)" Margin="5,2,5,2" Grid.Column="5">
                    <Grid>
                        <Grid.RowDefinitions>
                            <RowDefinition/>
                            <RowDefinition/>
                            <RowDefinition/>


                        </Grid.RowDefinitions>
                        <DockPanel>
                            <TextBlock Text="LABmin:" VerticalAlignment="Center" Foreground="#FFAE4141" FontWeight="Bold" ToolTip="IGF_RLT_Nachtbetrieb" Width="75" Height="18"/>
                            <TextBox x:Name="labmineingabe" IsEnabled="False" Width="70" Height="18" TextChanged="labmineingabe_changed" PreviewTextInput="Textinput0" HorizontalAlignment="Center" VerticalAlignment="Center"/>

                        </DockPanel>
                        <DockPanel Grid.Row="1">
                            <TextBlock Text="LABmax:" VerticalAlignment="Center" Foreground="#FFAE4141" FontWeight="Bold" ToolTip="IGF_RLT_Nachtbetrieb" Width="75" Height="18"/>
                            <TextBox x:Name="labmaxeingabe" IsEnabled="False" Width="70" Height="18" TextChanged="labmaxeingabe_changed" PreviewTextInput="Textinput0" HorizontalAlignment="Center" VerticalAlignment="Center"/>

                        </DockPanel>
                        <DockPanel Grid.Row="2" Visibility="Hidden">
                            <TextBlock Text="LABnacht:" VerticalAlignment="Center" Foreground="#FFAE4141" FontWeight="Bold" ToolTip="IGF_RLT_Nachtbetrieb" Width="75" Height="18"/>
                            <TextBox x:Name="labnachteingabe" IsEnabled="False" Width="70" Height="18" TextChanged="labnachteingabe_changed" PreviewTextInput="Textinput0" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                        </DockPanel>
                        <DockPanel Grid.Row="3" Visibility="Hidden">
                            <TextBlock Text="LABtnacht:" VerticalAlignment="Center" Foreground="#FFAE4141" FontWeight="Bold" ToolTip="IGF_RLT_Nachtbetrieb" Width="75" Height="18"/>
                            <TextBox x:Name="labtnachteingabe" IsEnabled="False" Width="70" Height="18" TextChanged="labtnachteingabe_changed" PreviewTextInput="Textinput0" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                        </DockPanel>
                        <DockPanel Grid.Row="4">
                            <TextBlock Text="24h-Ab:" VerticalAlignment="Center" Foreground="#FFAE4141" FontWeight="Bold" ToolTip="IGF_RLT_Nachtbetrieb" Width="75                                        " Height="18"/>
                            <TextBox x:Name="ab24heingabe" IsEnabled="False" Width="70" Height="18" TextChanged="ab24heingabe_changed" PreviewTextInput="Textinput0" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                        </DockPanel>
                    </Grid>
                </GroupBox>
            </Grid>



        </GroupBox>
        <StackPanel Grid.Column="2" x:Name="warnung" Grid.Row="1" VerticalAlignment="Center" Height="124">
            <TextBlock Text="Achtung: Der VSR versorgt mehrere Räume!" Foreground="#FFFF0505" TextWrapping="Wrap" VerticalAlignment="Top" Margin="10,0" FontSize="14"/>
            <TextBlock Text="-------------------------------------------------------------" VerticalAlignment="Top" Margin="10,0" FontSize="14"/>
            <TextBlock x:Name="Rauminfo" Text="Die angezeigte Rauminfomationen beziehen sich auf Raum =&gt;" TextWrapping="Wrap" Visibility="Visible" FontSize="14" Foreground="#FFF5085E" HorizontalAlignment="Stretch" VerticalAlignment="Center" Padding="10,0,0,0"/>
            <StackPanel Height="40"  Margin="10,0,10,10"  VerticalAlignment="Bottom">
                <TextBlock x:Name="raumwechsel_hinweis" Text="Hier den Raum wechseln:"/>
                <ComboBox x:Name="auswahl"  SelectionChanged="VSR_MEPRaum_changed"/>
            </StackPanel>
        </StackPanel>

        <DockPanel Margin="10,5,5,10" Grid.Row="2" >
            <TabControl  Background="LightBlue">
                <TabItem >
                    <TabItem.Header>
                        <TextBlock Text="Übersicht" FontWeight="Bold"/>
                    </TabItem.Header>
                    <DataGrid x:Name="grundinfo" Background="LightBlue" ItemsSource="{Binding}" CanUserSortColumns="False">
                        <DataGrid.CellStyle>
                            <Style TargetType="{x:Type DataGridCell}">
                                <Setter Property="ToolTip" Value="{Binding tooltip}"/>
                                <Style.Triggers>
                                    <DataTrigger Binding="{Binding name}" Value="24h Ab">
                                        <Setter Property="Foreground" Value="#FFC85B00"/>
                                    </DataTrigger>
                                    <DataTrigger Binding="{Binding name}" Value="min.RAB">
                                        <Setter Property="Foreground" Value="Orange"/>
                                    </DataTrigger>
                                    <DataTrigger Binding="{Binding name}" Value="min.AB_SUM">
                                        <Setter Property="Foreground" Value="Orange"/>
                                    </DataTrigger>
                                    <DataTrigger Binding="{Binding name}" Value="min.LAB">
                                        <Setter Property="Foreground" Value="orange"/>
                                    </DataTrigger>
                                    <DataTrigger Binding="{Binding name}" Value="min.RZU">
                                        <Setter Property="Foreground" Value="Blue"/>
                                    </DataTrigger>
                                    <DataTrigger Binding="{Binding name}" Value="min.ZU_SUM">
                                        <Setter Property="Foreground" Value="Blue"/>
                                    </DataTrigger>
                                    <DataTrigger Binding="{Binding name}" Value="max.RAB">
                                        <Setter Property="Foreground" Value="Orange"/>
                                    </DataTrigger>
                                    <DataTrigger Binding="{Binding name}" Value="max.AB_SUM">
                                        <Setter Property="Foreground" Value="Orange"/>
                                    </DataTrigger>
                                    <DataTrigger Binding="{Binding name}" Value="max.LAB">
                                        <Setter Property="Foreground" Value="Orange"/>
                                    </DataTrigger>
                                    <DataTrigger Binding="{Binding name}" Value="max.RZU">
                                        <Setter Property="Foreground" Value="Blue"/>
                                    </DataTrigger>
                                    <DataTrigger Binding="{Binding name}" Value="max.ZU_SUM">
                                        <Setter Property="Foreground" Value="Blue"/>
                                    </DataTrigger>
                                    <DataTrigger Binding="{Binding name}" Value="NB Zu">
                                        <Setter Property="Foreground" Value="Blue"/>
                                    </DataTrigger>
                                    <DataTrigger Binding="{Binding name}" Value="TNB Zu">
                                        <Setter Property="Foreground" Value="Blue"/>
                                    </DataTrigger>
                                    <DataTrigger Binding="{Binding name}" Value="NB Ab">
                                        <Setter Property="Foreground" Value="Orange"/>
                                    </DataTrigger>
                                    <DataTrigger Binding="{Binding name}" Value="TNB Ab">
                                        <Setter Property="Foreground" Value="Orange"/>
                                    </DataTrigger>



                                    <DataTrigger Binding="{Binding name}" Value="min.TZU">
                                        <Setter Property="Foreground" Value="Blue"/>
                                    </DataTrigger>
                                    <DataTrigger Binding="{Binding name}" Value="max.TZU">
                                        <Setter Property="Foreground" Value="Blue"/>
                                    </DataTrigger>
                                    <DataTrigger Binding="{Binding name}" Value="max.TAB">
                                        <Setter Property="Foreground" Value="Orange"/>
                                    </DataTrigger>
                                    <DataTrigger Binding="{Binding name}" Value="min.TAB">
                                        <Setter Property="Foreground" Value="Orange"/>
                                    </DataTrigger>
                                    <DataTrigger Binding="{Binding name}" Value="Überstrom">
                                        <Setter Property="Foreground" Value="DarkGreen"/>
                                    </DataTrigger>
                                    <DataTrigger Binding="{Binding name}" Value="Über. Ein.">
                                        <Setter Property="Foreground" Value="DarkGreen"/>
                                    </DataTrigger>
                                    <DataTrigger Binding="{Binding name}" Value="Über. Aus.">
                                        <Setter Property="Foreground" Value="DarkGreen"/>
                                    </DataTrigger>
                                    <DataTrigger Binding="{Binding name}" Value="Über.Ein.M.">
                                        <Setter Property="Foreground" Value="DarkGreen"/>
                                    </DataTrigger>
                                    <DataTrigger Binding="{Binding name}" Value="Über.Aus.M.">
                                        <Setter Property="Foreground" Value="DarkGreen"/>
                                    </DataTrigger>
                                </Style.Triggers>
                            </Style>
                        </DataGrid.CellStyle>
                        <DataGrid.Columns>
                            <DataGridTextColumn IsReadOnly="True" Header="Art" Width="*" Binding="{Binding name}"/>
                            <DataGridTextColumn IsReadOnly="True" Header="Soll (m³/h)" Width="*" Binding="{Binding Path= soll}"/>
                            <DataGridTextColumn IsReadOnly="True" Header="Redu. Soll (m³/h)" Width="*" Binding="{Binding Path= reduziert}"/>
                            <DataGridTextColumn IsReadOnly="True" Header="Ist (m³/h)" Width="*" Binding="{Binding Path= ist}"/>
                        </DataGrid.Columns>
                    </DataGrid>
                </TabItem>
                <TabItem >
                    <TabItem.Header>
                        <TextBlock Text="Detailsansicht" FontWeight="Bold"/>
                    </TabItem.Header>
                    <TabControl  Background="LightBlue">
                        <TabItem Header="TagMinbetrieb"  Width="100">
                            <ScrollViewer Background="White" Margin="0,0,0,0">
                                <StackPanel>


                                    <DockPanel Margin="0,5,0,0" >
                                        <TextBlock Text="" FontWeight="Bold" Width="100" Foreground="Blue"/>
                                        <TextBlock  FontWeight="Bold" Text="MEP" Width="100" />
                                        <TextBlock FontWeight="Bold" Text="Anlagen" Width="100" />
                                    </DockPanel>
                                    <Rectangle StrokeThickness="2" Stroke="Black" OpacityMask="Black"/>
                                    <DockPanel Margin="0,5,0,0" >
                                        <TextBlock Text="Zuluft-Summe: " FontWeight="Bold" Width="100" Foreground="Blue"/>
                                        <TextBlock x:Name="b_zu_min_sum_mep" FontWeight="Bold" Width="100" Foreground="Blue"/>
                                        <TextBlock x:Name="b_zu_min_sum_sys" FontWeight="Bold" Width="100" Foreground="Blue"/>
                                    </DockPanel>
                                    <Rectangle StrokeThickness="1" Stroke="Black" OpacityMask="Black"/>
                                    <DockPanel Margin="0,5,0,0" >
                                        <TextBlock Text="Zu-Raum: " Width="100"  Foreground="Blue"/>
                                        <TextBlock x:Name="b_zu_min_raum_mep" Width="100" Foreground="Blue"/>
                                        <TextBlock x:Name="b_zu_min_raum_sys" Width="100"   Foreground="Blue"/>
                                    </DockPanel>

                                    <Rectangle StrokeThickness="1" Stroke="Black" OpacityMask="Black"/>
                                    <DockPanel Margin="0,2,0,0" >
                                        <TextBlock Text="Zu-Überstrom: " Width="100"  Foreground="DarkGreen"/>
                                        <TextBlock x:Name="b_zu_min_ueber_mep" Width="100" Foreground="DarkGreen"/>
                                        <TextBlock x:Name="b_zu_min_ueber_sys" Width="100" Foreground="DarkGreen"/>
                                    </DockPanel>
                                    <Rectangle StrokeThickness="2" Stroke="Black" OpacityMask="Black"/>
                                    <DockPanel Margin="0,2,0,0" >
                                        <TextBlock Text="Zu-Überstrom_M: " Width="100"  Foreground="DarkGreen"/>
                                        <TextBlock x:Name="b_zu_min_ueber_m_mep" Width="100" Foreground="DarkGreen"/>
                                        <TextBlock x:Name="b_zu_min_ueber_m_sys" Width="100" Foreground="DarkGreen"/>
                                    </DockPanel>
                                    <Rectangle StrokeThickness="2" Stroke="Black" OpacityMask="Black"/>

                                    <DockPanel Margin="0,10,0,0" >
                                        <TextBlock Text="Abluft-Summe: " FontWeight="Bold" Width="100"  Foreground="Orange"/>
                                        <TextBlock x:Name="b_ab_min_sum_mep" FontWeight="Bold" Width="100" Foreground="Orange"/>
                                        <TextBlock x:Name="b_ab_min_sum_sys" FontWeight="Bold" Width="100" Foreground="Orange"/>
                                    </DockPanel>
                                    <Rectangle StrokeThickness="1" Stroke="Black" OpacityMask="Black"/>
                                    <DockPanel Margin="0,5,0,0" >
                                        <TextBlock Text="Ab-Raum: " Width="100" Foreground="Orange"/>
                                        <TextBlock x:Name="b_ab_min_raum_mep" Width="100"  Foreground="Orange"/>
                                        <TextBlock x:Name="b_ab_min_raum_sys" Width="100"  Foreground="Orange"/>
                                    </DockPanel>

                                    <Rectangle StrokeThickness="1" Stroke="Black" OpacityMask="Black"/>
                                    <DockPanel Margin="0,2,0,0" >
                                        <TextBlock Text="Ab-Überstrom: " Width="100" Foreground="Orange"/>
                                        <TextBlock x:Name="b_ab_min_ueber_mep" Width="100" Foreground="Orange"/>
                                        <TextBlock x:Name="b_ab_min_ueber_sys" Width="100" Foreground="Orange"/>
                                    </DockPanel>
                                    <Rectangle StrokeThickness="1" Stroke="Black" OpacityMask="Black"/>
                                    <DockPanel Margin="0,2,0,0" >
                                        <TextBlock Text="Ab-Überstrom_M: " Width="100" Foreground="Orange"/>
                                        <TextBlock x:Name="b_ab_min_ueber_m_mep" Width="100" Foreground="Orange"/>
                                        <TextBlock x:Name="b_ab_min_ueber_m_sys" Width="100" Foreground="Orange"/>
                                    </DockPanel>
                                    <Rectangle StrokeThickness="1" Stroke="Black" OpacityMask="Black"/>
                                    <DockPanel Margin="0,2,0,0" >
                                        <TextBlock Text="Ab-Labor: " Width="100" Foreground="Orange"/>
                                        <TextBlock x:Name="b_ab_min_lab_mep" Width="100" Foreground="Orange"/>
                                        <TextBlock x:Name="b_ab_min_lab_sys" Width="100" Foreground="Orange"/>
                                    </DockPanel>
                                    <Rectangle StrokeThickness="1" Stroke="Black" OpacityMask="Black"/>
                                    <DockPanel Margin="0,2,0,0" >
                                        <TextBlock Text="Ab-24h: " Width="100" Foreground="#FFC85B00"/>
                                        <TextBlock x:Name="b_ab_24h_min_mep" Width="100" Foreground="#FFC85B00"/>
                                        <TextBlock x:Name="b_ab_24h_min_sys" Width="100" Foreground="#FFC85B00"/>
                                    </DockPanel>
                                    <Rectangle StrokeThickness="2" Stroke="Black" OpacityMask="Black"/>
                                    <DockPanel Margin="0,10,0,0" >
